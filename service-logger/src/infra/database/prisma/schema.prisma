generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Purchase {
  id            String   @id @default(auto()) @map("_id") @db.ObjectId
  transactionId String
  log           String
  createdAt     DateTime @default(now()) @map("created_at")

  @@map("purchases")
}
